{
    "proxy": {
        "upstream": "<UPSTREAM URL>",
        "prefix": "/api",
        "enableHTTP2": false,
        "httpTimeout": 30000
    },
    "cookie": {
        "secret": "<COOKIE SECRET>",
        "parseOptions": {},
        "domain": "localhost",
        "path": "/",
        "tokenCookieName": "token",
        "expiryInDays": 30
    },
    "auth": {
        "clientId": "<OAUTH2 CLIENT ID>",
        "clientSecret": "<OAUTH2 CLIENT SECRET>",
        "redirectUrl": "/auth/callback",
        "discoveryDocumentUrl": "<OPEN ID PROVIDER .well-known CONFIGS>",
        "scopes": []
    }
}
